{
    "files.exclude": {
        ".editorconfig": true,
        ".github": true,
        "docs": true,
        "material": true,
        "Sources": true,
        "Testing": true,
        "wiki": true,
        "AppDeployToolkitLogo.png": true,
        "CHANGELOG.txt": true,
        "COPYING.Lesser": true,
        "LICENSE": true,
        "README.md": true,
        "Examples": true,
        ".gitattributes": true,
        ".gitignore": true,
        "mkdocs.yml": true,
        "pl.ps1": true
    },
    "terminal.integrated.profiles.windows": {
        "PowerShell": {
            "path": "C:\\Program Files\\PowerShell\\7\\pwsh.exe",
            "source": "PowerShell",
            "args": [
                "-NoLogo",
                "-NoExit",
                "-ExecutionPolicy",
                "RemoveSigned"
            ]
        },
        "Git Bash": null,
        "Command Prompt": null
    },
    "terminal.integrated.defaultProfile.windows": "PowerShell",
    "files.defaultLanguage": "powershell",
    "terminal.integrated.enablePersistentSessions": false,
    "terminal.integrated.persistentSessionReviveProcess": "never",
    "powershell.scriptAnalysis.settingsPath": ".vscode\\PSScriptAnalyzerSettings.psd1"
}
